{
  "compilerOptions": {
    "sourceMap": true,
    "target": "ESNext",
    "outDir": "./dist",
    "baseUrl": "./src",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "module": "CommonJS",
  },
  "include": ["src/**/*.ts", "utils/**/*.ts"],
  "exclude": ["node_modules"],
  
}
